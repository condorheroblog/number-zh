{
	"name": "number-to-zh",
	"version": "0.0.0",
	"type": "module",
	"license": "MIT",
	"author": "Condor Hero",
	"description": "阿拉伯数字转（大写，小写）简 / 繁体中文数字",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		}
	},
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"keywords": [
		"number-to-zh",
		"zh-to-number",
		"number-to-money",
		"money-to-number",
		"中文数字读法",
		"数字转中文数字",
		"中文转阿拉伯数字",
		"阿拉伯数字转中文数字",
		"阿拉伯数字转简体中文数字",
		"阿拉伯数字转小写简体中文数字",
		"阿拉伯数字转大写简体中文数字",
		"阿拉伯数字转繁体中文数字",
		"阿拉伯数字转大写繁体中文数字",
		"阿拉伯数字转小写繁体中文数字"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/condorheroblog/number-zh.git",
		"directory": "packages/number-to-zh"
	},
	"bugs": {
		"url": "https://github.com/condorheroblog/number-zh/issues"
	},
	"homepage": "https://github.com/condorheroblog/number-zh/blob/main/README.md",
	"scripts": {
		"prepublishOnly": "nr build",
		"test": "vitest",
		"dev": "esno ./src/numberToZh.ts",
		"build": "rimraf dist && rollup -c",
		"build:watch": "rimraf dist && rollup -c -w",
		"typecheck": "tsc --noEmit",
		"release": "bumpp -c 'release(number-to-zh): v%s' -t 'number-to-zh_v%s' && pnpm publish"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	}
}
